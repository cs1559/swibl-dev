// Check to ensure this file is included in Joomla!
defined('_JEXEC') or die( 'Restricted access' );

class JLUser extends fsBaseObject  {

	var $id = null;
	var $username = null;
	var $name = null;
	var $email = null;
		
	function __construct() {
    	parent::__construct();
    	$this->id = 0;
    }
    
    function setId($inParm) {
    	$this->id = $inParm;
    }
    function getId() {
    	return $this->id;
    }
    
	function getUsername() {
		return $this->username;
	}	
	function setUsername($name) {
		$this->username = $name;
	}
	
	function getName() {
		return $this->name;
	}
	function setName($name) {
		$this->name = $name;
	}
	function getEmail() {
		return $this->email;
	}
	function setEmail($email) {
		$this->email = $email;
	}
	
}
?>